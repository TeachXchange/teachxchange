<link rel="stylesheet" href="/css/feed-base.css">
<link rel="stylesheet" href="/css/feed-vendor.css">
<link rel="stylesheet" href="/css/feed-main.css"> 

<script src="/js/feed-modernizr.js"></script>
<script src="/js/feed-pace.min.js"></script>

<section class="s-content">
<div class="row">
    <div class="col-full">
        

        <form>
            <div class="form-row">
                <div class="form-group col-md-4">
                    <label for="inputState">Type of Upload</label>
                    <select id="inputState" class="form-control">
                        <option selected>Text</option>
                        <option>Link to Site</option>
                        <option>Picture</option>
                        <option>Video</option>
                        <option>Audio</option>

                    </select>
                </div>

                <div class="form-group col-md-8">
                    <label for="inputCity">Title</label>
                    <input type="text" class="form-control" id="inputCity">
                </div>
            </div>
            <div>
                <label for="title">Title</label>
                <input class="full-width" type="text" placeholder="Title" id="title">
            </div>

            <label for="exampleMessage">Message</label>
            <textarea class="full-width" placeholder="Your message" id="exampleMessage"></textarea>

            <div class="form-group">
                <label for="exampleInputFile">File input</label>
                <input type="file" class="form-control-file" id="exampleInputFile" aria-describedby="fileHelp">
            </div>
        </form>
    </div>
</div>

</section>
